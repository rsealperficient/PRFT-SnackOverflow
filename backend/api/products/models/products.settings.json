{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "listings": {
      "via": "product",
      "collection": "listing"
    },
    "rarity": {
      "type": "enumeration",
      "enum": [
        "Common",
        "Rare",
        "Uncommon",
        "Mythic"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "richtext"
    },
    "set": {
      "type": "enumeration",
      "enum": [
        "Kaldheim",
        "Vow"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Creature",
        "Instant",
        "Sorcery",
        "Enchantment",
        "Land",
        "Artifact"
      ]
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "products"
    }
  }
}
